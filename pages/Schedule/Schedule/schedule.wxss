.checkbox{
    width: 33%;height: auto;
    margin: 10rpx 0 10rpx 0;
    text-align: left;font-size:28rpx; 
    overflow: hidden;display: inline-block;
}

.submit{
    width:676rpx;height: 74rpx;
    background: #e23738;
    color: #fff;border: none;outline: none;
    font-size: 28rpx;border-radius: 0;
    margin-top: 20rpx;
}

.tips{
    font-size: 22rpx;color:#666565;
    margin-top: 10rpx;text-align: center;
}
